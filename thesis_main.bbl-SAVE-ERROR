    \entry{Mattioli2023}{inbook}{}{}
      \name{author}{3}{}{%
        {{hash=fefbbbfde907d4c91e6f9f2ce7e9627a}{%
           family={Mattioli},
           familyi={M\bibinitperiod},
           given={Giulio},
