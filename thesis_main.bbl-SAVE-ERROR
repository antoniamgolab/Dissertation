    \entry{CHATTERTON201830}{article}{}{}
      \name{author}{4}{}{%
        {{hash=c2ef88eb7b62f93f9f237b50e31ee5e4}{%
           family={Chatterton},
           familyi={C\bibinitperiod},
           given={T.},
