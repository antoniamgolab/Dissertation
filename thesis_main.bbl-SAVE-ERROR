    \entry{Baresch2019}{article}{}{}
      \name{author}{2}{}{%
        {{hash=7d6a6ee29f342b64366b6c5ad3e8c8cc}{%
           family={Baresch},
           familyi={B\bibinitperiod},
           given={Martin},
           giveni={M\bibinitperiod}}}%
        {{hash=afbc155270659b710c961743f553c8d5}{%
