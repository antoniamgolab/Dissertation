      \strng{namehash}{b0a744b2455f72dc1a784d8a86ef4efb}
      \strng{fullhash}{87c0d1409a26e1466498b430e06f1a7d}
      \strng{fullhashraw}{87c0d1409a26e1466498b430e06f1a7d}
      \strng{bibnamehash}{87c0d1409a26e1466498b430e06f1a7d}
      \strng{authorbibnamehash}{87c0d1409a26e1466498b430e06f1a7d}
      \strng{authornamehash}{b0a744b2455f72dc1a784d8a86ef4efb}
      \strng{authorfullhash}{87c0d1409a26e1466498b430e06f1a7d}
